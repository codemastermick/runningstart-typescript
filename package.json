{
  "name": "runningstart-typescript",
  "version": "0.0.1",
  "description": "A library to make your libraries!",
  "main": "dist/index.js",
  "bin": {
    "init": "bin/init.js",
    "add-feature": "bin/add-feature.js"
  },
  "keywords": [
    "typescript",
    "generator",
    "scaffold",
    "library"
  ],
  "author": "Codemaster Mick",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codemastermick/runningstart-typescript"
  },
  "scripts": {
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:coverage": "rimraf coverage",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "build": "tsc",
    "build:docs": "typedoc --out docs src",
    "docs": "run-s clean:docs build:docs",
    "gh-pages": "gh-pages -d docs",
    "watch:test": "mocha --watch",
    "watch:dev": "tsnd --respawn --transpile-only ./src/index.ts",
    "publish:docs": "npm run docs gh-pages",
    "dev": "run-s clean format lint watch:dev",
    "prestart": "run-s clean:dist build",
    "start": "./dist/index.js",
    "test": "mocha",
    "test:coverage": "c8 npm test"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  }
}
